@model List<Vidly.Models.Movie>
<h2>Movies</h2>
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th scope="col">Movies</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>
                    @movie.Name
                </td>
            </tr>
          }
    </tbody>

</table>
